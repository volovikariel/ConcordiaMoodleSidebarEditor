{
  "name": "Moodle Dashboard Organizer",
  "version": "0.6.9",
  "description": "Toggle visibility for your Concordia Moodle courses",
  "content_scripts": [
    {
      "matches": [
        "https://moodle.concordia.ca/*"
      ],
      "js": [
        "content_script.js"
      ],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "manifest_version": 3
}